{
  "version": 2,
  "buildCommand": "bash build.sh",
  "builds": [
    {
      "src": "api/socket.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/socket\\.io(.*)",
      "dest": "/api/socket.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/socket.js"
    },
    {
      "src": "(.*)",
      "dest": "/api/socket.js"
    }
  ]
}