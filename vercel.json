{
  "version": 2,
  "buildCommand": "bash build.sh",
  "public": "client/dist",
  "builds": [
    {
      "src": "api/socket.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/socket\\.io(.*)",
      "dest": "/api/socket.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/socket.js"
    },
    {
      "src": "/(.*)",
      "methods": ["GET", "HEAD"],
      "dest": "/client/dist/$1"
    }
  ]
}