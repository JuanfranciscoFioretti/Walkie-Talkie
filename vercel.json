{
  "version": 2,
  "builds": [
    {
      "src": "build.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "client/dist"
      }
    }
  ],
  "functions": {
    "api/socketio.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/socket.io/(.*)",
      "dest": "/api/socketio"
    },
    {
      "src": "/api/socketio",
      "dest": "/api/socketio"
    },
    {
      "src": "/(.*)",
      "dest": "/client/dist/$1"
    }
  ]
}